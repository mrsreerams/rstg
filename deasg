#include <stdio.h>
// function to perform insertion sort
void insertionsort(int arr[], int n){
    int i, key, j;
    for(i=1;i<n;i++){
        key=arr[i];
        j=i-1;
        while(j>=0 &&arr[j]>key){
        arr[j+1]=arr[j];
        j=j-1;}
        arr[j+1]=key;
    }
}
void printarray(int arr[], int size){
    int i;
    for(i=0;i<size;i++)
        printf(" %d ",arr[i]);
    printf(" \n ");
}
int main(){
    int n;
    printf("Enter the number of the elemens:");
    scanf("%d",&n);
    int arr[n];
    printf("Enter the elements of the array:\n");
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    printf("Original array: \n");
    printarray(arr,n);
    insertionsort(arr,n);
    printf("\n Sorted array: \n");
    printarray(arr, n);
    return 0;
}
